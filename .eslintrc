{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "lines-around-comment": 1,
    "newline-after-var": 1,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "prefer-template": 0,
    "quote-props": 0,
    "spaced-comment": 1,
    "strict": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module-alias": {}
    }
  }
}
